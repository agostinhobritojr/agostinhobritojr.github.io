#include <iostream>

using namespace std;

class Vetor2d{
private:
  float x, y;
public:
  void setX(float _x){
	x = _x;
  }
  float getX(void){
	return x;
  }
  void setY(float _y){
	y = _y;
  }
  float getY(void){
	return y;
  }
};

int main(void){
  Vetor2d v1;
  v1.setX(4.5);
  v1.setY(-1.3);
  cout << v1.getX() << " " << v1.getY() << "\n";
}
